nav {
  width: 100%;
  background-color: #4682B4;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
nav .nav-links {
  list-style: none;
}
nav .nav-links .nav-item a {
  display: inline-block;
  text-decoration: none;
  color: white;
  padding: 1em 2em;
}
nav .nav-links .nav-item a:hover {
  color: #abc;
}
nav .nav-links .nav-item a.active {
  font-weight: bold;
}
nav .nav-container, aside .aside-container {
  position: sticky;
  top: 20px;
  padding: 2em;
}
nav h2, aside h2 {
  border-bottom: 2px solid #666;
}
nav ul {
  list-style: none;
  margin-left: 0.5rem;
}
aside ul {
  margin-left: 1em;
}
nav ul li, aside ul li {
  position: relative;
  margin: 20px 0;
}
nav ul li a {
  line-height: 2em;
  text-transform: uppercase;
  text-decoration: none;
  letter-spacing: 0.4em;
  display: block;
  transition: all ease-out 300ms;
}
nav ul li.active a {
  color: #444;
  font-weight: 900;
}
nav ul li:not(.active) a {
  color: #666;
  transition: all 0.3s ease;
}
nav ul li:not(.active):hover a {
  color: #444;
}
nav ul li:not(.active):after {
  opacity: 0.2;
}
nav ul li:after {
  content: "";
  position: absolute;
  width: 100%;
  height: 2px;
  background: black;
  left: 0;
  bottom: 0;
  background-image: linear-gradient(to right, #FFFFFF, #FFD700);
  transition: all 0.3s ease;
}
nav ul li:not(.active):hover:after {
  opacity: 0.5;
}
